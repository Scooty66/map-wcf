<?xml version="1.0"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTD/options.dtd">
<data>
	<import>
		<categories>
			<category name="profile.map">
				<icon>icon/glob24.png</icon>
				<parent>profile</parent>
				<showorder>15</showorder>
			</category>
		</categories>
		<options>
			<option name="map_enable">
				<categoryname>profile.map</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>map_street,map_zip,map_city,map_country</enableoptions>
				<visible>4</visible>
			</option>
			<option name="map_street">
				<categoryname>profile.map</categoryname>
				<optiontype>text</optiontype>
				<visible>4</visible>
			</option>
			<option name="map_zip">
				<categoryname>profile.map</categoryname>
				<optiontype>text</optiontype>
				<visible>4</visible>
			</option>
			<option name="map_city">
				<categoryname>profile.map</categoryname>
				<optiontype>text</optiontype>
				<visible>4</visible>
				<searchable>1</searchable>
			</option>
			<option name="map_country">
				<categoryname>profile.map</categoryname>
				<optiontype>text</optiontype>
				<visible>4</visible>
			</option>
			<option name="map_coord">
				<categoryname>hidden</categoryname>
				<optiontype>text</optiontype>
				<visible>4</visible>
				<editable>4</editable>
			</option>
			<option name="map_lastlookup">
				<categoryname>hidden</categoryname>
				<optiontype>text</optiontype>
				<visible>4</visible>
				<editable>4</editable>
			</option>
		</options>
	</import>
</data>
