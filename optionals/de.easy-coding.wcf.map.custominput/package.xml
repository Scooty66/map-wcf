<?xml version="1.0"?>
<!DOCTYPE package SYSTEM "http://www.woltlab.com/DTDs/package.dtd">
<package name="de.easy-coding.wcf.map.custominput">
	<packageinformation>
		<packagename>Google Maps: Custom Inputfields</packagename>
		<packagedescription><![CDATA[This plugin allows the admin to define custom useroptions which can be used for the google maps query.]]></packagedescription>
		<isunique>1</isunique>
		<version>1.0.0</version>
		<date>DATE</date>
		<plugin>com.woltlab.wcf.page.user.profile</plugin>
	</packageinformation>

	<authorinformation>
		<author>Torben Brodt</author>
		<authorurl>http://trac.easy-coding.de/trac/wcf</authorurl>
	</authorinformation>

	<requiredpackages>
		<requiredpackage minversion="1.1.0">com.woltlab.wcf</requiredpackage>
		<requiredpackage minversion="2.0.5">de.gmap.wcf.data.page.map</requiredpackage>
		<requiredpackage minversion="1.0.4">com.woltlab.wcf.page.user.profile</requiredpackage>
	</requiredpackages>

	<instructions type="install">
		<files>files.tar</files>
		<acptemplates>acptemplates.tar</acptemplates>
		<options>options.xml</options>
		<eventlistener>eventlistener.xml</eventlistener>
		<languages languagecode="de">de.xml</languages>
		<languages languagecode="en">en.xml</languages>
	</instructions>
	
	<instructions type="update" fromversion="1.0.0 Beta 1">
		<files>files.tar</files>
        </instructions>
</package>
